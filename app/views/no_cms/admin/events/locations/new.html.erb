<% content_for :second_menu do %>
  <%= render partial: 'location_listing_item', collection: @locations, as: 'location', layout: 'location_listing' unless @locations.blank? %>
<% end %>


<%= no_cms_admin_form_for @location do |f| %>
  <%= render partial: 'no_cms/admin/shared/toolbar', locals: { f: f } %>
  <div class="content">
    <%= render 'no_cms/admin/shared/log_messages' %>
    <%= render 'content_fields', f: f %>
  </div>
<% end %>
