<% content_for :second_menu do %>
  <%= render partial: 'event_listing_item', collection: @events, as: 'event', layout: 'event_listing', locals: { current: @event } unless @events.blank? %>
<% end %>


<%= no_cms_admin_form_for @event do |f| %>
  <%= render partial: 'no_cms/admin/shared/toolbar', locals: { f: f } %>
  <div class="content">
    <%= render 'no_cms/admin/shared/log_messages' %>
    <%= render 'content_fields', f: f %>
  </div>
<% end %>
